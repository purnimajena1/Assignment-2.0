     "text": [
      "input:please insert comma separated number:20,30,40,50,60,70\n",
      "output list: ['20', '30', '40', '50', '60', '70']\n"
     ]
    }
   ],
   "source": [
    "number_list = []\n",
    "number = input('input:please insert comma separated number:')\n",
    "for i in number.split(','):\n",
